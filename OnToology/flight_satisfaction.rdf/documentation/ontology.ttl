<http://www.semanticweb.org/usuari/ontologies/2024/3/flight_satisfaction> rdf:type owl:Ontology ;
                                                                           rdfs:comment "Ontology that represents the answers to flight satisfaction surveys."@en-us .

#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/usuari/ontologies/2024/3/flight_satisfaction#carries
:carries rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdf:type owl:AsymmetricProperty ;
         rdfs:domain :Aircraft ;
         rdfs:range :Author ,
                    :Passenger ,
                    :Person ,
                    [ rdf:type owl:Restriction ;
                      owl:onProperty :livesIn ;
                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                      owl:onClass :Country
                    ] ;
         rdfs:comment "An aircraft carries a passenger."@en-us ;
         rdfs:label "carries" .


###  http://www.semanticweb.org/usuari/ontologies/2024/3/flight_satisfaction#describes
:describes rdf:type owl:ObjectProperty ;
           owl:inverseOf :isDescribedBy ;
           rdfs:domain :Review ;
           rdfs:range :Flight .


###  http://www.semanticweb.org/usuari/ontologies/2024/3/flight_satisfaction#hasConnection
:hasConnection rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               owl:inverseOf :isConnectionOf ;
               rdf:type owl:AsymmetricProperty ;
               rdfs:domain :Flight ,
                           [ rdf:type owl:Restriction ;

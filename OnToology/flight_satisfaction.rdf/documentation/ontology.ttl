                             owl:onClass :Aircraft
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasConnection ;
                             owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass :Airport
                           ] ;
               rdfs:range :Airport ;
               rdfs:comment "A flight has a connection on this airport."@en-us ;
               rdfs:label "hasConnection" .


